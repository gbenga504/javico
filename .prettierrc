{
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": ["src/**/*.js", "src/**/*.ts", "src/**/*.tsx"],
      "options": {
        "trailingComma": "all",
        "tabWidth": 2,
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "jsxBracketSameLine": true
      }
    }
  ]
}